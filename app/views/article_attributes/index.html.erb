<p id="notice"><%= notice %></p>

<h1>Article Attributes</h1>

<table>
  <thead>
    <tr>
      <th>Value</th>
      <th>References</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @article_attributes.each do |article_attribute| %>
      <tr>
        <td><%= article_attribute.value %></td>
        <td><%= article_attribute.references %></td>
        <td><%= link_to 'Show', article_attribute %></td>
        <td><%= link_to 'Edit', edit_article_attribute_path(article_attribute) %></td>
        <td><%= link_to 'Destroy', article_attribute, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Article Attribute', new_article_attribute_path %>
