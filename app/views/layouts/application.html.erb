<!DOCTYPE html>
<html>
  <head>
    <title>Winted</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <header>
        <%= render '/layouts/navbar' %>
    </header>
    <div>
      <%= render '/layouts/messages' %>
    </div>
    <%= yield %>
  </body>
</html>
